<h1> STUDENTS!!! </h1>



<ul>
	<% @students.each do |student| %>
		<li>
			<span> <%= link_to(student.first_name, student_path(student)) %> </span> | 
			<span> <%= student.last_name %> </span> | 
			<span> <%= student.email %> </span> | 
			<span> <%= student.phone %> </span>	
		</li>
	<% end %>
</ul>
